/**
 * Problem 6
 */
package com.test;

public class Test6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		final int N = 100;
		int num1 = 0;
		int num2 = 0;
		for(int i=0;i<N;i++){
			num1 = num1 + (i+1)*(i+1);
		}
		for(int i=0;i<N;i++){
			num2 = num2 + (i+1);
		}
		num2 = num2 * num2;
		System.out.println(num2-num1);
	}

}
